      "transluscentBg": true,
      "navigation": [
        {
          "close": "close_window_button",
					"moveWindow": "header"
         
        }
      ]
      
    }
  ],

  "QSettings": [
    {
      "ThemeSettings": [
        {
          "CustomTheme": [
            {
              "Background-color": "#F5F5F5",
