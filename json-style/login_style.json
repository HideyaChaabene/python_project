      ],
      "AppSettings": {
        "OrginizationName": "FlowCaptor",
        "ApplicationName": "FlowCaptor",
        "OrginizationDormain": "FlowCaptor.org"
      }
    }
  ],
  "QCustomQStackedWidget":[
		{
			"name":"index_stack",
			"navigation":[
				{
					"navigationButtons":[
						{
							"to_signup": "welcome_signup_pg",
							"to_login": "welcome_login_pg"
						}
					]
				}
			],
      "transitionAnimation":[
				{
					"slide":[
						{
							"active":true,
							"duration": 1000,
							"direction": "horizontal",
							"easingCurve": "Linear"
						}
					]
				}
			]
		},
    {
			"name":"form_stack",
			"navigation":[
				{
					"navigationButtons":[
						{
							"to_signup": "signup_pg",
							"to_login": "login_pg",
							"rest": "Rest_pwd"
						}
					]
				}
			],
      "transitionAnimation":[
				{
					"slide":[
						{
							"active":true,
							"duration": 1000,
							"direction": "vertical",
							"easingCurve": "Linear"
						}
					]
				}
			]
		}
	],
	"QCustomCheckBox":[
		{
			"name":"checkBox",
			"bgColor":"#B7B7B7",
			"circleColor":"#008080",
			"activeColor":"#99CCCC",
			"animationEasingCurve":"Linear",
			"animationDuration":1000
		}
	],
	"QCustomSlideMenu": [
		{
				"name": "notificationSlide",
				"defaultSize": [
						{
								"width": 800,
								"height": 40
						}
				],
				"collapsedSize": [
						{
								"width": 800,
								"height": 0
						}
				],
				"expandedSize": [
						{
								"width": 800,
								"height": "auto"
						}
				],
				"toggleButton": [
						{
								"buttonName": "closeNotification"
						}
				],
				"menuTransitionAnimation": [
						{
								"animationDuration": 500,
								"animationEasingCurve": "Linear"
						}
				]
		}
]
}
