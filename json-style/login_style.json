              "Theme-name": "Modern Teal Slate",
              "Default-Theme": true
            },
						{
              "Background-color": "#333333",
              "Text-color": "#F5F5F5",
              "Accent-color": "#005555",
              "Icons-color": "#F5F5F5",
              "Theme-name": "Midnight Teal Slate",
              "Default-Theme": false
            }
          ]
